FROM us.icr.io/dia-registry/devops/build-117:latest as build

WORKDIR $GOPATH

WORKDIR $GOPATH/src/
COPY ./cmd/blockchain/ethereum/diaUsdlrOracleService ./

RUN go install

FROM gcr.io/distroless/base

COPY --from=build /go/bin/diaUsdlrOracleService /bin/diaUsdlrOracleService
COPY --from=build /config/ /config/

CMD ["diaUsdlrOracleService"]
