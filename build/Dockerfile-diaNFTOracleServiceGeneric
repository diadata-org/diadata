FROM us.icr.io/dia-registry/devops/build:latest as build

WORKDIR $GOPATH

WORKDIR $GOPATH/src/
COPY ./cmd/blockchain/ethereum/nft/diaGenericNftOracleService ./

RUN go install

FROM gcr.io/distroless/base

COPY --from=build /go/bin/diaGenericNftOracleService /bin/diaGenericNftOracleService
COPY --from=build /config/ /config/

CMD ["diaGenericNftOracleService"]
